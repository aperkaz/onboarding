{
  "name": "onboarding",
  "version": "0.1.0",
  "description": "Provides information about campaigns and it'c contacts",
  "scripts": {
    "start": "node src/server/index.js",
    "dev": "npm run setup-consul ; ${NODE_PATH}/.bin/nodemon --watch src/server src/server/index.js",
    "test": "npm run clean ; npm run setup-consul ; ${NODE_PATH}/.bin/nyc ${NODE_PATH}/.bin/mocha --recursive -R mocha-junit-reporter",
    "clean": "${NODE_PATH}/.bin/rimraf .nyc_output coverage test-results.xml",
    "setup-consul": "curl -X PUT -d ${MYSQL_DATABASE} http://${HOST_IP}:8500/v1/kv/onboarding/db-init/database ; curl -X PUT -d 'root' http://${HOST_IP}:8500/v1/kv/onboarding/db-init/user ; curl -X PUT -d ${MYSQL_ROOT_PASSWORD} http://${HOST_IP}:8500/v1/kv/onboarding/db-init/password ; curl -X PUT -d 'true' http://${HOST_IP}:8500/v1/kv/onboarding/db-init/populate-test-data ; curl -X PUT -d ${REDIS_AUTH} http://${HOST_IP}:8500/v1/kv/onboarding/redis-auth",
    "rest-doc": "mkdir -p wiki/rest-doc ; ${NODE_PATH}/.bin/raml2md -o file-per-resource -p wiki/rest-doc --output-ext=.endpoint.md rest-doc/main.raml",
    "domain-doc": "mkdir -p wiki/domain-doc ; ${NODE_PATH}/.bin/sq2md -o file-per-src -p wiki/domain-doc --init=init --field-bl=createdAt,updatedAt --output-ext=.model.md src/server/db/models",
    "api-doc": "mkdir -p wiki/api-doc ; ${NODE_PATH}/.bin/jsdoc2md -f $(find src/server) > wiki/api-doc/Home.md",
    "doc": "npm run rest-doc ; npm run domain-doc ; npm run api-doc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/OpusCapita/onboarding"
  },
  "author": "Daniel Zhitomirsky",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/OpusCapita/onboarding/issues"
  },
  "homepage": "https://github.com/OpusCapita/onboarding/blob/master/README.md",
  "dependencies": {
