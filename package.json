{
  "name": "onboarding",
  "version": "0.1.0",
  "description": "Provides information about campaigns and it'c contacts",
  "scripts": {
    "start": "node src/server/index.js",
    "dev": "npm run setup-consul ; ${NODE_PATH}/.bin/nodemon --watch src/server src/server/index.js",
    "test": "npm run clean ; npm run setup-consul ; ${NODE_PATH}/.bin/nyc ${NODE_PATH}/.bin/mocha --recursive -R mocha-junit-reporter",
    "clean": "${NODE_PATH}/.bin/rimraf .nyc_output coverage test-results.xml",
    "setup-consul": "curl -X PUT -d 'bnp' http://${HOST_IP}:8500/v1/kv/onboarding/db-init/database ; curl -X PUT -d 'root' http://${HOST_IP}:8500/v1/kv/onboarding/db-init/user ; curl -X PUT -d 'notSecurePa55w0rd' http://${HOST_IP}:8500/v1/kv/onboarding/db-init/password",
    "rest-doc": "mkdir -p wiki/rest-doc ; ${NODE_PATH}/.bin/raml2md -o file-per-resource -p wiki/rest-doc --output-ext=.endpoint.md rest-doc/main.raml",
    "domain-doc": "mkdir -p wiki/domain-doc ; ${NODE_PATH}/.bin/sq2md -o file-per-src -p wiki/domain-doc --init=init --field-bl=createdAt,updatedAt --output-ext=.model.md src/server/db/models",
    "api-doc": "mkdir -p wiki/api-doc ; ${NODE_PATH}/.bin/jsdoc2md -f $(find src/server) > wiki/api-doc/Home.md",
    "doc": "npm run rest-doc ; npm run domain-doc ; npm run api-doc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/OpusCapita/onboarding"
  },
  "author": "Daniel Zhitomirsky",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/OpusCapita/onboarding/issues"
  },
  "homepage": "https://github.com/OpusCapita/onboarding/blob/master/README.md",
  "dependencies": {
    "async": "2.1.5",
    "bluebird": "3.0.5",
    "bluebird-retry": "0.10.1",
    "body-parser": "1.15.2",
    "cookie-parser": "1.4.3",
    "jquery": "2.1.4",
    "mysql": "2.13.0",
    "node-schedule": "1.2.1",
    "nodemailer": "2.7.2",
    "nodemon": "1.11.0",
    "ocbesbn-config": "2.x",
    "ocbesbn-db-init": "1.x",
    "ocbesbn-web-init": "0.x",
    "opuscapita-i18n": "1.0.8",
    "papaparse": "4.1.2",
    "polyfill-promise": "4.0.1",
    "react-bootstrap-table": "2.5.7",
    "react-dropzone": "3.7.3",
    "react-intl": "2.2.3",
    "react-notification-system": "0.2.13",
    "recharts": "0.20.5",
    "redux-form": "6.5.0",
    "redux-thunk": "2.2.0",
    "rimraf": "2.6.1",
    "superagent": "2.3.0",
    "superagent-bluebird-promise": "4.2.0",
    "umzug": "1.11.0",
    "uuid": "3.0.1",
    "validate.js": "0.10.0",
    "xlsx": "0.9.1"
  },
  "devDependencies": {
    "babel-plugin-istanbul": "3.0.0",
    "babel-plugin-transform-object-assign": "6.8.0",
    "babel-plugin-webpack-loaders": "0.8.0",
    "chai": "3.5.0",
    "command-line-args": "4.0.2",
    "jsdoc-to-markdown": "3.0.0",
    "mocha": "3.2.0",
    "mocha-junit-reporter": "1.12.1",
    "nodemon": "1.11.0",
    "npm-run-all": "4.0.1",
    "nyc": "10.1.2",
    "raml-to-markdown": "1.x",
    "sequelize-to-markdown": "1.x",
    "webpack": "2.3.2",
    "webpack-dev-middleware": "1.8.4",
    "webpack-hot-middleware": "2.13.1"
  }
}
