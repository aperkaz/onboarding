{
  "extends": "opuscapita",
  "rules": {
    "react/prop-types": 0,
    "guard-for-in": 0,
    "react/prefer-stateless-function": 2,
    "no-mixed-spaces-and-tabs": [2, "smart-tabs"],
    "max-len": [2, 120, 2, {
      "ignoreUrls": true,
      "ignoreComments": false,
      "ignoreTemplateLiterals": true
    }]
  }
}
