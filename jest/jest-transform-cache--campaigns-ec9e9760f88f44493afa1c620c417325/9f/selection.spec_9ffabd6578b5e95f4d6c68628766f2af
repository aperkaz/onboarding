'use strict';var _selection = require('../../../client/actions/campaignContacts/selection');var actions = _interopRequireWildcard(_selection);
var _campaignContacts = require('../../../client/constants/campaignContacts');


var _lodash = require('lodash');var _lodash2 = _interopRequireDefault(_lodash);function _interopRequireDefault(obj) {return obj && obj.__esModule ? obj : { default: obj };}function _interopRequireWildcard(obj) {if (obj && obj.__esModule) {return obj;} else {var newObj = {};if (obj != null) {for (var key in obj) {if (Object.prototype.hasOwnProperty.call(obj, key)) newObj[key] = obj[key];}}newObj.default = obj;return newObj;}}

it('calls request and success actions if the fetch response was successful', function () {
  var expectedSelectAction = actions.selectContact('ff', 'karan@cronj.com');
  var expectedRemoveAction = actions.removeSelection();

  expect(expectedSelectAction).toEqual({
    type: _campaignContacts.SELECT_CAMPAIGN_CONTACT,
    email: 'karan@cronj.com',
    campaignId: 'ff' });


  expect(expectedRemoveAction).toEqual({ type: _campaignContacts.REMOVE_CAMPAIGN_CONTACT_SELECTION });
});